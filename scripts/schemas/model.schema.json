{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "name",
    "parameters_billions",
    "hidden_size",
    "num_layers",
    "num_heads",
    "default_context_length",
    "architecture"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z0-9-]+$"
    },
    "name": {
      "type": "string"
    },
    "parameters_billions": {
      "type": "number",
      "minimum": 0.1,
      "maximum": 1000
    },
    "hidden_size": {
      "type": "number",
      "minimum": 128,
      "maximum": 32768
    },
    "num_layers": {
      "type": "number",
      "minimum": 1,
      "maximum": 200
    },
    "num_heads": {
      "type": "number",
      "minimum": 1,
      "maximum": 256
    },
    "default_context_length": {
      "type": "number",
      "minimum": 512,
      "maximum": 1000000
    },
    "architecture": {
      "type": "string",
      "enum": ["transformer", "moe"]
    },
    "num_kv_heads": {
      "type": "number",
      "minimum": 1,
      "maximum": 256
    },
    "intermediate_size": {
      "type": "number",
      "minimum": 128
    },
    "vocab_size": {
      "type": "number",
      "minimum": 1000
    },
    "num_experts": {
      "type": "number",
      "minimum": 2
    },
    "experts_per_token": {
      "type": "number",
      "minimum": 1
    }
  }
}
